{
    "name": "get-things-done/nova-permission",
    "description": "A Laravel Nova tool for Spatie's Permission library.",
    "keywords": [
        "laravel",
        "nova",
        "spatie-permission"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "spatie/laravel-permission": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "GetThingDone\\NovaPermission\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        },
        "laravel": {
            "providers": [
                "GetThingDone\\NovaPermission\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
